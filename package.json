{
  "name": "a",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "dev:json-server": "json-server __json-server__/db.json --port 3100",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "generate:dts": "yarn dtsmake -s ",
    "deploy": "yarn push-dir --dir=dist --branch=gh-pages --cleanup",
    "deploy:all": "yarn generate && yarn deploy",
    "lint": "lint:js",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "prepare": "husky install",
    "format": "prettier --write .",
    "test": "jest"
  },
  "config": {
    "nuxt": {
      "host": "0.0.0.0",
      "port": "80"
    }
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{md,css,scss,ts}": "yarn format"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/proxy": "^2.1.0",
    "@tailwindcss/forms": "^0.3.4",
    "classnames": "^2.3.1",
    "cookie-universal-nuxt": "^2.1.5",
    "core-js": "^3.15.1",
    "element-ui": "^2.15.2",
    "nuxt": "^2.15.7",
    "qs": "^6.10.1",
    "vue-lazyload": "^1.3.3",
    "vue-tailwind": "^2.5.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/style-resources": "^1.2.1",
    "@nuxtjs/tailwindcss": "^4.2.1",
    "@vue/test-utils": "^1.2.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^27.0.5",
    "daisyui": "^1.16.2",
    "dtsmake": "^0.0.10",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.12.1",
    "fibers": "^5.0.0",
    "husky": "^6.0.0",
    "jest": "^27.0.5",
    "json-server": "^0.17.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.3.2",
    "push-dir": "^0.4.1",
    "sass": "^1.43.4",
    "sass-loader": "10",
    "vue-jest": "^3.0.4"
  }
}
